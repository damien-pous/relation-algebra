DECLARE PLUGIN "rocq-relation-algebra.mrewrite"

{
  open Ltac_plugin
  open Stdarg
  open Tacarg
  open Mrewrite
}

TACTIC EXTEND ra_extend_lr
  | [ "ra_extend" tactic(k) "->" constr(h) ] -> { extend ist k `LR h } END
TACTIC EXTEND ra_extend_rl 
  | [ "ra_extend" tactic(k) "<-" constr(h) ] -> { extend ist k `RL h } END
